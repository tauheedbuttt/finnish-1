  "topic": "possessivesentences",
  "title": "Omistusrakenne - Possessive Sentences",
  "description": "Practice 'I have' sentences with -lla/-ll√§ and partitive in negatives",
  "instructions": "Form correct possessive sentences using -lla/-ll√§",

  "rules": {
    "basic_structure": {
      "description": "To say 'X has Y' in Finnish, use: X + -lla/-ll√§ + on + Y",
      "formula": "OWNER + -lla/-ll√§ + on + THING",
      "examples": [
        "Minulla on auto = I have a car",
        "Sinulla on koira = You have a dog",
        "Liisalla on kirja = Liisa has a book"
      ]
    },
    "pronouns": {
      "description": "Add -lla/-ll√§ to pronouns (spoken forms)",
      "table": {
        "min√§": "minulla / mulla",
        "sin√§": "sinulla / sulla",
        "h√§n": "h√§nell√§ / sill√§",
        "me": "meill√§",
        "te": "teill√§",
        "he": "heill√§ / niill√§"
      }
    },
    "names_nouns": {
      "description": "Add -lla/-ll√§ to names and nouns",
      "vowel_harmony": "Back vowels (a,o,u) ‚Üí -lla | Front vowels (√§,√∂,y) + neutral (e,i) ‚Üí -ll√§",
      "examples": [
        "Liisa ‚Üí Liisalla",
        "Matti ‚Üí Matilla",
        "√§iti ‚Üí √§idill√§ (KPT: t‚Üíd)",
        "is√§ ‚Üí is√§ll√§"
      ]
    },
    "adjectives": {
      "description": "Adjectives describing the owner also get -lla/-ll√§",
      "examples": [
        "Pienell√§ lapsella on lelu = The small child has a toy",
        "Vanhalla miehell√§ on koira = The old man has a dog",
        "Uudella autolla on hyv√§t renkaat = The new car has good tires"
      ]
    },
    "negative": {
      "description": "Negative sentences use partitive for the object",
      "formula": "X + -lla/-ll√§ + ei ole + PARTITIVE",
      "examples": [
        "Minulla ei ole autoa = I don't have a car",
        "Sinulla ei ole koiraa = You don't have a dog",
        "Liisalla ei ole rahaa = Liisa doesn't have money"
      ],
      "note": "Remember: ei ole + partitive!"
    },
    "feelings": {
      "description": "Express feelings/states with possessive structure",
      "common_feelings": [
        "n√§lk√§ (hunger) - Minulla on n√§lk√§ = I'm hungry",
        "jano (thirst) - Minulla on jano = I'm thirsty",
        "kiire (hurry) - Minulla on kiire = I'm in a hurry",
        "kylm√§ (cold) - Minulla on kylm√§ = I'm cold",
        "kuuma (hot) - Minulla on kuuma = I'm hot",
        "v√§symys/v√§synyt (tired) - Minulla on v√§synyt = I'm tired",
        "ik√§v√§ (longing) - Minulla on ik√§v√§ = I miss (someone)"
      ],
      "negative_feelings": "Feelings in negative do NOT use partitive!",
      "negative_examples": [
        "Minulla ei ole n√§lk√§ = I'm not hungry (NOT n√§lk√§√§)",
        "Minulla ei ole jano = I'm not thirsty (NOT janoa)",
        "Minulla ei ole kiire = I'm not in a hurry (NOT kiirett√§)"
      ]
    }
  },
